{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended","plugin:react/recommended", "prettier"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react", "prettier"],
  "rules": {
    "semi": 2,
    "react/prop-types": "off",
    "prettier/prettier": "error"
  }
}
