{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "esnext",
    "target": "es5",
    "jsx": "react",
    "sourceMap": true,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "Node",
    "alwaysStrict":true,
    "noImplicitReturns":true,
    "noUnusedParameters":true
  },
  "include": [
    "./src/"
  ],
  "exclude": [
    "node_modules"
  ]
}


